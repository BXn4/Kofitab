{
  "manifest_version": 2,
  "name": "Kofitab",
  "version": "0.1",
  "description": "Your new homepage. Highly customizable and easy to use.",
  "icons": {
    "48": "icons/icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "home.html"
  },
  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "geolocation"
  ]
}